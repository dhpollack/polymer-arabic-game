{
  "name": "tinavg: Alphabets for Everyone",
  "short_name": "tinavg",
  "manifest_version": 2,
  "version": "0.2.3",
  "description": "Alphabets for Everyone!  A simple, fast-paced, fun pattern matching language game.",
  "icons": { "192": "launcher-icon.png" },
  "author": "David Pollack",
  "app": {
    "background": {
      "scripts": [ "background.js" ],
      "persistent": false
    }
  },
  "externally_connectable": {
    "matches": ["*://*.firebase.com/*", "*://*.firebaseio.com/*"]
  },
  "permissions": [
    "https://cdn.firebase.com/",
    "https://*.firebaseio.com/",
    "https://fonts.googleapis.com/"
  ],
  "sandbox": {
    "pages": ["index.html"],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'"

  }
}